#include <stdio.h>

int main() {
    int groups[] = {1, 3, 5, 3, 1};
    int num_groups = sizeof(groups) / sizeof(groups[0]);
    for(int g = 0; g < num_groups; g++) {
        for(int s = 0; s < groups[g]; s++) {
            printf("*\n");
        }
        if(g != num_groups - 1)
            printf("\n"); // Print a blank line except after last group
    }
    return 0;
}
